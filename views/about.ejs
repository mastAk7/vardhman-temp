    .timeline::before {
      content: '';
      position: absolute;
      left: 50%;
      top: 0;
      bottom: 0;
      width: 3px;
      background: linear-gradient(to bottom, #E2E8F0, #CBD5E0);
      transform: translateX(-50%);
      border-radius: 2px;
