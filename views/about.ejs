    .timeline-item {
      margin-bottom: 60px;
      position: relative;
      opacity: 0;
      transform: translateY(20px);
      transition: all 0.6s ease;
    }

    .timeline-item.visible {
      opacity: 1;
      transform: translateY(0);
    }

    .timeline-item:nth-child(odd) {
      padding-right: 50%;
      padding-left: 0;
    }

    .timeline-item:nth-child(even) {
      padding-left: 50%;
      padding-right: 0;
